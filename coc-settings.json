{
  "python.pythonPath": "${workspaceFolder}/.venv/bin/python",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "${workspaceFolder}/.venv/bin/black",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "pyright.organizeimports.provider": "isort",
  "pyright.organizeimports.path": "${workspaceFolder}/.venv/bin/isort",
  "pyright.organizeImport.args": ["--profile", "black"],
  "yaml.format.enable": true,
  "coc.preferences.rootPatterns": [
    ".git",
    "pyproject.toml",
    "setup.py",
    ".env"
  ],
  "coc.preferences.formatOnSaveFiletypes": ["python", "go", "yaml", "json"],
  "coc.preferences.willSaveHandlerTimeout": 3000,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "●",
  "diagnostic.hintSign": "○",
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true,
    "borderhighlight": "WarningMsg"
  },
  "markdownlint.config": {
    "default": true,
    "line_length": false
  },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "cSpell.userWords": [
    "BBAN",
    "BEEZUP",
    "Heikemen",
    "Isort",
    "Munif",
    "OCTOPIA",
    "ODATA",
    "SELLO",
    "Sanic",
    "Typer",
    "Vesuna",
    "anyio",
    "asdict",
    "asyncio",
    "christoomey",
    "consts",
    "dhruvasagar",
    "fannheyward",
    "fyndiq",
    "gopls",
    "gtin",
    "hashivim",
    "iban",
    "kyazdani",
    "levouh",
    "lxml",
    "machakann",
    "markdownlint",
    "minikube",
    "neoclide",
    "onupdate",
    "organizeimports",
    "pascalize",
    "pydantic",
    "pydocstring",
    "pylint",
    "pyproject",
    "pyright",
    "qlty",
    "sasl",
    "scrooloose",
    "selectin",
    "serialisers",
    "statsd",
    "strtobool",
    "structlog",
    "svlarna",
    "thaerkh",
    "tmpl",
    "unpublish",
    "uvicorn",
    "venv"
  ]
}
