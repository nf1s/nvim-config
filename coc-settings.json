{
  "python.pythonPath": "${workspaceFolder}/.venv/bin/python",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "${workspaceFolder}/.venv/bin/black",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "pyright.organizeimports.provider": "isort",
  "python.sortImports.path": "${workspaceFolder}/.venv/bin/isort",
  "python.sortImports.args": ["--profile", "black"],
  "yaml.format.enable": true,
  "workspace.rootPatterns": [".git", "pyproject.toml", "setup.py", ".env"],
  "coc.preferences.formatOnSave": true,
  "coc.preferences.willSaveHandlerTimeout": 3000,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "●",
  "diagnostic.hintSign": "○",
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true,
    "borderhighlight": "WarningMsg"
  },
  "markdownlint.config": {
    "default": true,
    "line_length": false
  },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "cSpell.userWords": [
    "BBAN",
    "BEEZUP",
    "Fyndians",
    "Heikemen",
    "Isort",
    "Munif",
    "OCTOPIA",
    "ODATA",
    "Recogniton",
    "SELLO",
    "Sanic",
    "Typer",
    "Vesuna",
    "anyio",
    "asdict",
    "asyncio",
    "borderhighlight",
    "chatgpt",
    "christoomey",
    "consts",
    "dhruvasagar",
    "fannheyward",
    "fastavro",
    "fyndiq",
    "gcloud",
    "gopls",
    "gtin",
    "hashivim",
    "iban",
    "isawaitable",
    "iscoroutine",
    "iscoroutinefunction",
    "kubectl",
    "kubernetes",
    "kyazdani",
    "lazygit",
    "levouh",
    "lxml",
    "machakann",
    "markdownlint",
    "minikube",
    "neoclide",
    "noack",
    "onupdate",
    "organizeimports",
    "pascalize",
    "pydantic",
    "pydocstring",
    "pylint",
    "pymongo",
    "pyproject",
    "pyright",
    "qlty",
    "sasl",
    "scrooloose",
    "selectin",
    "serialisers",
    "skaffold",
